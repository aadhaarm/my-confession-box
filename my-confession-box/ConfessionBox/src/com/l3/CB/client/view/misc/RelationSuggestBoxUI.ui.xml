<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel styleName="uk-form-row">
		<g:SuggestBox ui:field="relationSuggestBox" styleName="uk-width-1-1" />
		<g:ListBox ui:field="relationSuggestList" styleName="uk-width-1-1" />
		<span class="uk-text-info" ui:field="spanMessage">Relation of this person to
			you. (example: friend, brother, wife, uncle, boss etc)</span>
	</g:HTMLPanel>
</ui:UiBinder>