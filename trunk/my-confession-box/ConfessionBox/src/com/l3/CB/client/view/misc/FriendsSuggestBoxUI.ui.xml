<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel styleName="uk-form-row">
		<g:Image ui:field="imgFriend"/>
		<g:SuggestBox ui:field="friendsSuggestBox" styleName="uk-width-1-1"/>
		<g:ListBox ui:field="friendsSuggestList" styleName="uk-width-1-1"/>
		<span class="uk-text-info" ui:field="spanMessage">Type your friend's name and choose from the options</span>
	</g:HTMLPanel>
</ui:UiBinder> 